ipt>\nexport default {\n  props: {\n    value: {\n      type: String,\n      default: \"\"\n    }\n  },\n  data: function() {\n    return {\n      count: 0,\n      isToMore: false\n    };\n  },\n  methods: {\n    handleCopy() {\n      if (this.value) {\n        this.$message({\n          message: \"复制成功\",\n          type: \"success\"\n        });\n      }\n    },\n    handleInput(vl) {\n      let value = vl.target.value;\n      value = value.trim();\n      this.count = value.length;\n      this.isToMore = value.length > 128;\n\n      value = value.replace(/\\s+/g, \" \");\n      this.$emit(\"input\", value);\n    }\n  }\n};\n</script>\n\n<style lang=\"less\">\n</style>"]}]}